<Application x:Class="BingWallpaper.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BingWallpaper"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/CircleButton.xaml"/>
                <ResourceDictionary Source="Styles/BlueButton.xaml"/>
                <ResourceDictionary Source="Styles/LightButton.xaml"/>
                <ResourceDictionary Source="Styles/SettingButton.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
